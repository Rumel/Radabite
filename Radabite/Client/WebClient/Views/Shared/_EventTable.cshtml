@using Radabite.Backend.Database;

@model List<Event>

<tbody>
	@{
		if (Model.Count() == 0)
		{
			<tr><td>You have no Events :(</td></tr>
		}
		else
		{
				foreach (var item in Model){
                <tr>
                    <td>
                        @Html.ActionLink(item.Title, "../Event", new { eventId = item.Id })
                    </td>
                </tr>
            }
		}
	}
</tbody>